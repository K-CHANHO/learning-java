package javatest;

public class StringOperation {

	public static void main(String[] args) {
		
		System.out.println("Hello World".length());
		System.out.println("Hello, leezche... bye. ".replace("leezche","eoing"));
		System.out.println("Hello, [[[name]]]... bye. ".replace("[[[name]]]","eoing"));

	}

}
